{
  "name": "jsdoc-tests",
  "version": "1.2.0",
  "description": "A JSDoc test tool for documentation-driven quality",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "scripts": {
    "build": "microbundle -f es,cjs",
